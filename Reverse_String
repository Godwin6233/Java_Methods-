public class name {

 
public static String reverseString( String x){
    
    
    String result = "";
    
    for (int i = x.length() - 1; i > 0; i--){
        result = result + x.charAt(i);
    }
    return result;


}
public static void main(String[] args){
       String x = "takeout";
       System.out.println(reverseString(x));
       
}
}
